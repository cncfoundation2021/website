{
  "manufacturing-of-products": {
    "title": "Manufacturing Request",
    "description": "Request manufacturing services for food products",
    "sections": [
      {
        "title": "Product Details",
        "fields": [
          {
            "name": "product_type",
            "label": "Product Type",
            "type": "select",
            "required": true,
            "options": [
              "Sweets",
              "Namkeen",
              "Puffed Rice",
              "Flattened Rice",
              "Other"
            ]
          },
          {
            "name": "quantity",
            "label": "Quantity (kg/pieces)",
            "type": "text",
            "required": true,
            "placeholder": "e.g., 100 kg, 500 pieces"
          },
          {
            "name": "packaging_requirements",
            "label": "Packaging Requirements",
            "type": "textarea",
            "required": false,
            "placeholder": "Specify packaging type, size, materials needed"
          }
        ]
      },
      {
        "title": "Production Details",
        "fields": [
          {
            "name": "production_timeline",
            "label": "Production Timeline",
            "type": "select",
            "required": true,
            "options": [
              "Within 1 week",
              "1-2 weeks",
              "2-4 weeks",
              "1-2 months",
              "Flexible"
            ]
          },
          {
            "name": "quality_standards",
            "label": "Quality Standards",
            "type": "select",
            "required": true,
            "options": [
              "Standard Quality",
              "Premium Quality",
              "Export Quality",
              "Custom Specifications"
            ]
          },
          {
            "name": "custom_ingredients",
            "label": "Custom Ingredients/Recipe",
            "type": "textarea",
            "required": false,
            "placeholder": "Any specific ingredients or recipe requirements"
          }
        ]
      }
    ]
  },

